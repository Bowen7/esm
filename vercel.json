{
  "routes": [
    { "src": "/vercel.svg", "dest": "/public/vercel.svg" },
    { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
    { "src": "/_next/.*", "dest": "/_next/" },
    { "src": "/.+", "dest": "/api/index.ts" }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": "node_modules/@swc/**"
    }
  }
}
